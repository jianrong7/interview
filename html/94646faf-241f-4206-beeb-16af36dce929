<script type="text/javascript">
	function toIndonesianDate(d) {
		var d_names = new Array("Minggu", "Senin", "Selasa", "Rabu", "Kamis", "Jumat", "Sabtu");
		var m_names = new Array("Januari", "Februari", "Maret", "April", "Mei", "Juni", "Juli", "Agustus", "September", "Oktober", "November", "Desember");
		var curr_day = d.getDay();
		var curr_date = d.getDate();
		var curr_month = d.getMonth();
		var curr_year = d.getFullYear();
		var ret = d_names[curr_day] + ", " +  curr_date + " " + m_names[curr_month] + " " + curr_year;
		return ret;
 	}
	
	function forceDisplayMenu() {
		if ( window.innerWidth > 768 ) {
			document.getElementsByClassName( "site-menu" )[0].style.display="inherit";
		}
	}

	$( ".html-banner-300x250 .edition").html( toIndonesianDate(new Date()));
  
    /*
	document.getElementsByClassName( "date-text" )[0].innerHTML = toIndonesianDate(new Date());
	document.getElementsByClassName( "article-date" )[0].innerHTML = toIndonesianDate( new Date( document.getElementsByClassName( "article-date" )[0].innerHTML ) );
    */
	document.getElementsByClassName( "date-text" )[0].innerHTML = toIndonesianDate( new Date( document.getElementsByClassName( "article-date" )[0].innerHTML ) );
	document.getElementsByClassName( "article-date" )[0].innerHTML = '';
    
    
	window.onresize = forceDisplayMenu;
	
	/*
	-------------------------------------------------------------------------
	code di atas untuk menggantikan code berbasis jquery yang tidak jalan ini
