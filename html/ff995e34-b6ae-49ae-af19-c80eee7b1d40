	$(window).resize(function() {
		if ( $(window).width() > 768 ) {
			$('.site-menu').show();
		}
	});
	$(".date-text").html( getCurrDateString() );

	if ($('.datepicker').length > 0) {
		$('.datepicker').datepicker();
	}

